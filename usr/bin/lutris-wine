[ -z "$LW_CFG" ] && export LW_CFG="$LW_DEF_CFG"
[ -z "$VKBASALT_CONFIG_FILE" ] && export VKBASALT_CONFIG_FILE="$LW_DIR/vkBasalt.conf"
[ -z "$LW_RESHADE_DIR" ] && export LW_RESHADE_DIR="$LW_RUNTIME_DIR/reshade"
[ -z "$LW_DEF_ICO" ] && export LW_DEF_ICO="/usr/share/lutris-wine/icon/default_icon.png"
