[ -z "$LW_DEF_CACHE" ] && export LW_DEF_CACHE="$LW_DIR/cache"
[ -z "$LW_DEF_CENV" ] && export LW_DEF_CENV="$LW_DIR/luwine.cenv"
[ -z "$LW_CENV" ] && export LW_CENV="$LW_DEF_CENV"
[ -z "$LW_APPCFG_DIR" ] && export LW_APPCFG_DIR="$LW_DIR/appcfg"
[ -z "$VLK_ICD_DIR" ] && export VLK_ICD_DIR="/usr/share/vulkan/icd.d"
[ -z "$SYS_WINE" ] && export SYS_WINE="$(which -a wine 2>/dev/null|grep -v "$(basename "$LW_DIR")"|head -1)"
[ -z "$SYS_LU" ] && export SYS_LU="$(which lutris 2>/dev/null)"
export > /tmp/envbackup$ENVKEY

