            --field="NO WRITE WATCH:CHK" "$NO_WRITE_WATCH" \
            --field="VULKAN NO ASYNC:CHK" "$VULKAN_NO_ASYNC" \
            --field="HIDE NVIDIA GPU:CHK" "$HIDE_NVIDIA_GPU" \
