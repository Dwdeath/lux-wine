check_us_switcher() {
    if [ -z "$US_LAYOUT" ]
        then
            export US_LAYOUT=1
    fi
    if [[ "$US_LAYOUT" == 1 && -z "$(which xkb-switch 2>/dev/null)" ]]
        then
            export US_LAYOUT=0
            sed -i "s/export US_LAYOUT.*/export US_LAYOUT='0'/g" "$LW_CFG" 2>/dev/null
